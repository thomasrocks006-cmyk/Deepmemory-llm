# Google Cloud / Gemini API
GOOGLE_API_KEY=your_gemini_api_key_here
GOOGLE_CLOUD_PROJECT=your_project_id
VERTEX_AI_LOCATION=us-central1

# Pinecone Vector Database
PINECONE_API_KEY=your_pinecone_api_key
PINECONE_ENVIRONMENT=us-west1-gcp
PINECONE_INDEX_NAME=deepmemory-vectors

# Neo4j Graph Database
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password

# PostgreSQL Database
DATABASE_URL=postgresql://user:password@localhost:5432/deepmemory
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# Application Settings
APP_ENV=development
DEBUG=true
LOG_LEVEL=INFO
SECRET_KEY=your_secret_key_for_jwt

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Frontend URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# Context Caching
CONTEXT_CACHE_TTL=3600
MAX_CACHE_SIZE=1048576

# Agent Configuration
THINKING_LEVEL=high
MAX_OUTPUT_TOKENS=8192
TEMPERATURE_CONSERVATIVE=0.3
TEMPERATURE_BALANCED=0.7
TEMPERATURE_CREATIVE=1.0

# Learning Loop
REFLECTION_INTERVAL=5
NIGHTLY_REFLECTION_TIME=02:00

# Feature Flags
ENABLE_MULTI_AGENT_CONSENSUS=false
ENABLE_PROACTIVE_INSIGHTS=true
ENABLE_VOICE_INTERFACE=false
